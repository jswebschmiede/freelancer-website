---
import { getCollection } from 'astro:content';
import { Icon } from 'astro-icon/components';
import Section from '@components/Ui/Section.astro';
import Card from '@components/Ui/Card.astro';
import PageLayout from '@layouts/PageLayout.astro';
import Glow from '@components/Ui/Glow.astro';
import Button from '@components/Ui/Button.astro';
import Dots from '@components/Ui/Dots.astro';
import ContactItem from '@components/Ui/ContactItem.astro';
import Project from '@components/Project/Project.astro';
import Faq from '@components/Faq.astro';
import Hero from '@components/Hero.astro';
import MagicCard from '@components/Ui/MagicCard.astro';

const allProjects = await getCollection('projects');
allProjects.sort((a, b) => (a.data.date < b.data.date ? 1 : -1));

const meta = {
  title: 'Jörg Schöneburg - Full-Stack Entwickler in Altenburg | Erfahrung & Leidenschaft',

  description:
    'Erfahren Sie mehr über Jörg Schöneburg, einen leidenschaftlichen Full-Stack Entwickler aus Altenburg mit umfangreicher Expertise. Entdecken Sie maßgeschneiderte Web-Lösungen für Ihr Projekt',
};
---

<PageLayout {meta} bodyClass="home">
  <Hero slot="hero" />

  <Section
    id="leistungen"
    className="bg-[radial-gradient(circle_at_30%_50%,var(--color-primary)_-200%,transparent_25%),radial-gradient(circle_at_70%_60%,var(--color-accent)_-200%,transparent_20%)]"
  >
    <div
      class="reveal-fx--translate-up reveal-fx prose mx-auto max-w-7xl px-4 text-center lg:prose-lg"
    >
      <h2 class="text-3xl font-bold md:text-3xl lg:text-5xl">
        <Dots />Meine <span class="text-primary">Leistungen</span>
      </h2>
      <p>
        Ich biete Ihnen maßgeschneiderte Web-Lösungen für Ihr Projekt. Egal ob Website, CMS oder ein
        individuelles System, ich unterstütze Sie bei der Umsetzung.
      </p>
    </div>

    <div
      class="reveal-fx--translate-up cards-wrap reveal-fx relative z-10 mx-auto mb-6 mt-24 grid max-w-7xl grid-cols-1 gap-8 px-4 sm:grid-cols-2 lg:grid-cols-3"
    >
      <MagicCard title="Card title!" icon="mdi:cellphone-text">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
      <MagicCard title="Card title!" icon="mdi:star-outline">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
      <MagicCard title="Card title!" icon="mdi:auto-fix">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
      <MagicCard title="Card title!" icon="mdi:thunder-outline">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
      <MagicCard title="Card title!" icon="mdi:code-block-braces">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
      <MagicCard title="Card title!" icon="mdi:checkbox-marked-circle-outline">
        <p>If a dog chews shoes whose shoes does he choose?</p>
      </MagicCard>
    </div>
  </Section>

  <Section id="prozess">
    <div
      class="reveal-fx--translate-up reveal-fx relative z-10 mx-auto grid max-w-7xl grid-cols-1 gap-20 px-4 lg:grid-cols-12"
    >
      <div class="lg:col-span-5">
        <div class="prose sticky top-24 lg:prose-lg">
          <h3 class="text-3xl font-bold md:text-3xl lg:text-5xl">
            <Dots className="mx-0" />Workflow
          </h3>
          <p>
            Ich entwickle moderne und performante Webseiten und Web-Apps. Dabei setze ich auf
            aktuelle Technologien und Frameworks.
          </p>
          <div
            class="mt-9 flex flex-col items-start justify-start gap-8 sm:flex-row sm:items-center"
          >
            <Button href="#kontakt" variant="outline"> Anfrage </Button>
            <Button href="#fragen" className="group">
              Fragen? <Icon
                name="mdi:arrow-right"
                class="w-5 h-5 group-hover:translate-x-2 transition-transform duration-200 will-change-transform"
              />
            </Button>
          </div>
        </div>
      </div>

